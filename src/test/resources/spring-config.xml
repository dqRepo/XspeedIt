<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<context:component-scan base-package="com.oui.sncf" />

	<context:annotation-config />

	<bean id="ItemBox" class="com.oui.sncf.items.ItemBox" />

	<bean id="Main" class="com.oui.sncf.main.Main" />
	
	<bean id="ItemsGenerator" class="com.oui.sncf.items.ItemsGenerator" />
	
	<bean id="BoxFiller" class="com.oui.sncf.items.BoxFiller" />
	
	<bean id="AppRunner" class="com.oui.sncf.applicationRunner.AppRunner" />

</beans>
